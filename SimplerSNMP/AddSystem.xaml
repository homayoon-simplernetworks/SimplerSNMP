<Window x:Class="SimplerSNMP.AddSystem"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SimplerSNMP"
        mc:Ignorable="d"
        Title="AddSystem" Height="300" Width="300">
    <Grid>
        <Button x:Name="button" Content="ADD and Close" HorizontalAlignment="Left" Margin="104,227,0,0" VerticalAlignment="Top" Width="117" Click="button_Click"/>
        <TextBox x:Name="ipTextBox" HorizontalAlignment="Left" Height="23" Margin="122,50,0,0" TextWrapping="Wrap" Text="192.168.3.110" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="portTestbox" HorizontalAlignment="Left" Height="23" Margin="122,78,0,0" TextWrapping="Wrap" Text="161" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="desTextbox" HorizontalAlignment="Left" Height="23" Margin="122,106,0,0" TextWrapping="Wrap" Text="dessss" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="rcTextbox" HorizontalAlignment="Left" Height="23" Margin="122,134,0,0" TextWrapping="Wrap" Text="public" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="wcTextbox" HorizontalAlignment="Left" Height="23" Margin="122,162,0,0" TextWrapping="Wrap" Text="AdminPublic" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
